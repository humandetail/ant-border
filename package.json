{
  "name": "ant-border",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "keywords": [
    "ant-border",
    "canvas",
    "draggable",
    "resizable",
    "ui"
  ],
  "description": "An ant border ui base on canvas",
  "types": "dist/index.d.ts",
  "main": "dist/ant-border.js",
  "module": "dist/ant-border.js",
  "browser": "dist/ant-border.cjs",
  "exports": {
    ".": {
      "import": "./dist/ant-border.js",
      "require": "./dist/ant-border.cjs"
    }
  },
  "files": [
    "dist/*"
  ],
  "unpkg": "dist/ant-border.js",
  "jsdelivr": "dist/ant-border.js",
  "homepage": "https://github.com/humandetail/ant-border",
  "repository": {
    "type": "git",
    "url": "https://github.com/humandetail/ant-border"
  },
  "bugs": {
    "url": "https://github.com/humandetail/ant-border/issues"
  },
  "scripts": {
    "dev": "vite serve example",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "release": "release-it"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^5.1.1",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.0.1",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^7.0.0",
    "lint-staged": "^13.2.1",
    "release-it": "^15.5.1",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-dts": "^2.2.0"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix"
  }
}
